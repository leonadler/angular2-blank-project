{
  "compileOnSave": false,
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "removeComments": false,
    "declaration": false,
    "allowJs": true,
    "noEmitHelpers": true,
    "sourceMap": false
  },
  "exclude": [
    "gulpfile.js",
    "karma.*",
    "*.conf.js",
    "*.config.js",
    "gulp",
    "node_modules",
    "build"
  ]
}
